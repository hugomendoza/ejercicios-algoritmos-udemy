Algoritmo sin_titulo
	
	Definir valdia, salario Como Real;
	Definir diatrab Como Entero;
	
	Escribir "Ingrese el valor x día trabajado";
	Leer valdia;
	
	Escribir "Ingrese la cantidad de días trabajados";
	Leer diatrab;
	
	salario = valdia * diatrab;
	
	Imprimir "Sueldo mensual; $/", salario;
	
FinAlgoritmo
