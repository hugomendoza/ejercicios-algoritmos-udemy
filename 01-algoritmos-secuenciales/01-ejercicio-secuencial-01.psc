Algoritmo sin_titulo
	
	Definir nombre, edad, ciudad Como Caracter;
	Escribir  "Ingrese el nombre de la persona";
	Leer nombre;
	
	Escribir "Ingrese le edad de la persona";
	Leer edad;
	
	Escribir  "Ingrese el nombre de su ciudad natal";
	Leer ciudad;
	
	Imprimir  "Tu nombre es ", nombre, " tienes ", edad, " de edad " "y tu ciudad natal es " ciudad;
	
FinAlgoritmo
