Proceso sin_titulo
	
	Definir a, b, suma, rest, mult, div Como Real;
	
	Escribir "Ingrese el primer número";
	Leer a;
	
	Escribir "Ingrese el segundo número";
	Leer b;
	
	suma = a + b;
	rest = a - b;
	mult = a * b;
	div = a / b;
	
	Imprimir  "Resultados";
	Imprimir "Suma: ", suma;
	Imprimir "Resta: ", rest;
	Imprimir "Multiplicación: ", mult;
	Imprimir "División: ", div;
	
FinProceso
